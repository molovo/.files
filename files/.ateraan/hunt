#class {hunt} {open}
  #alias {sp} {suckerpunch}
  #alias {lb} {lykosbite}
  #alias {bf} {backfist}
  #alias {rh} {roundhouse}
  #alias {ca} {cripplearm}
  #alias {ds} {dustspray}
  #alias {sd} {sojdodge}
  #alias {p}  {poultice}
  #alias {qh} {quickhand}
  #alias {ec} {eat cookie}
  #alias {ee} {eat erb}
  #alias {eq} {eat quella}
  #alias {eb} {eat bread}
  #alias {eg} {eat gatortail}
  #alias {dm} {drink mana}
  #alias {de} {drink elixir}
  #alias {dd} {drink drink}

  #alias {att} {
    ca %1;
    attack %1;
  }

  #alias {aaa} {
    suckerpunch;
    #delay {1} {
      lykosbite;
      #delay {1} {
        backfist;
      }
    }
  }

  #alias {aab} {
    roundhouse;
  }

  #alias {bbb} {
    sojdodge;
    #delay {1} {
      quickhand;
      #delay {1} {
        dustspray;
      }
    }
  }

  #alias {ccc} {
    read scroll;
    #delay {1} {
      aim wand at %1;
      #delay {1} {
        dart %1;
      }
    }
  }

  #alias {lance} {
    unwield staff;
    put bow in scabbard 2;
    put sword in scabbard 2;
    put dagger in scabbard 3;
    get lance from scabbard;
    wield lance;
    wear shield;
  }

  #alias {bow} {
    unwield staff;
    put lance in scabbard;
    get bow from scabbard 2;
    wield bow
  }

  #alias {sword} {
    unwield staff;
    unwear shield;
    put lance in scabbard;
    get sword from scabbard 2;
    get dagger from scabbard 3;
    wield sword;
    wield dagger;
  }

  #alias {shield} {
    put dagger in scabbard 3;
    wear shield;
  }

  #alias {dagger} {
    get dagger from scabbard 3;
    unwear shield;
    wield dagger;
  }

  #alias {uw} {
    unwield staff;
    put lance in scabbard;
    put sword in scabbard 2;
    put dagger in scabbard 3;
  }

  #alias {getskins} {
    get skins;
    put skins in backpack;
  }

  #alias {gethorns} {
    get horns;
    put horns in backpack;
  }

  #alias {fw} {
    firebow west at %1;
  }
  #alias {fe} {
    firebow east at %1;
  }
  #alias {fn} {
    firebow north at %1;
  }
  #alias {fs} {
    firebow south at %1;
  }
#class {hunt} {close}
