alias start='brew services start'
alias stop='brew services stop'
alias restart='brew services restart'
alias more='vim'
alias vi='vim'
alias h='history'
alias j='jobs -l'
alias path='echo -e ${PATH//:/}'
alias now='date +"%T"'
alias nowtime=now
alias nowdate='date +"%d-%m-%Y"'
alias git='hub'
alias top='sudo htop'
alias flushdns='sudo dscacheutil -flushcache;sudo killall -HUP mDNSResponder'
alias sudo='sudo env "PATH=$PATH"'
alias show-hidden='defaults write com.apple.finder AppleShowAllFiles YES; killall Finder /System/Library/CoreServices/Finder.app'
alias hide-hidden='defaults write com.apple.finder AppleShowAllFiles NO; killall Finder /System/Library/CoreServices/Finder.app'
alias gh='gh-home'
alias up='vagrant up && vagrant ssh'
alias reup='vagrant destroy && vagrant up && vagrant ssh'
alias gl='git log --decorate --graph --oneline --date=short --format="%C(auto)%h %C(auto)%d%n%C(5)%an %Cgreen%ad %Creset%s"'
alias wl="watson log"
alias wr="watson report"
alias ws="watson start"
alias wst="watson stop"
alias wrs="watson restart"
alias we="watson edit"
alias wf="watson frames"
alias wss="watson status"
alias wrm="watson remove"
alias tmux="tmux -u"
alias sql='mycli'
alias phake='phake -q'
alias mux='tmuxinator'
alias nwa='telnet www.ateraan.com 4002'
alias l='k -ha'
alias restart-pf='sudo launchctl unload -w /Library/LaunchDaemons/com.molovo.pfnginx.plist; sudo launchctl load -w /Library/LaunchDaemons/com.molovo.pfnginx.plist'
alias a='atom-beta'
alias labels='github-label-sync --labels ~/labels.json $(git config remote.origin.url | sed "s/https\:\/\/github.com\///g")'
alias tt='cd ~/.ateraan; tt++'
