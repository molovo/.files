"*":
  "api-docs":
    _theme: "Dark"
  "atom-alignment": {}
  "atom-beautify":
    general:
      _analyticsUserId: "a21ed3b1-d369-4457-876a-de8d9cfc3a6d"
  "atom-ctags":
    GotoSymbolKey: [
      "ctrl"
    ]
    autoBuildTagsWhenActive: true
    buildTimeout: 30000
    cmdArgs: "--exclude=.git --exclude=.svn --exclude=node_modules --exclude=.DS_Store"
  "atom-fuzzy-grep": {}
  "atom-itunes":
    cycleTrackInfo: true
  "atom-oss-license":
    name: "James Dinsdale <hi@molovo.co>"
  "auto-copyright":
    owner: "James Dinsdale"
    template: '''
      Copyright (c) %y James Dinsdale (hi@molovo.co). All Rights Reserved.
      
      Licensed under the MIT license.
    '''
  "autocomplete-ctags": {}
  "autocomplete-modules": {}
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    confirmCompletion: "tab"
    defaultProvider: "Fuzzy"
  blame: {}
  "coffee-compile":
    compileOnSaveWithoutPreview: true
    destination: "./public_html/scripts"
    focusEditorAfterCompile: true
    noTopLevelFunctionWrapper: false
    split: "Down"
  "color-picker":
    preferredFormat: "HEX"
  core:
    disabledPackages: [
      "linter-phpcs"
      "language-gfm"
      "atom-jade"
      "tree-view-breadcrumb"
      "tree-view-autoresize"
      "tree-view-filter"
      "tree-view-big-files"
      "tree-view-copy-relative-path"
      "linter-coffee-variables"
      "atom-bracket-highlight"
      "fancy-bracket-matcher"
      "synced-sidebar"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "node_modules"
      "._*"
      "Thumbs.db"
      ".tags*"
    ]
    packagesWithKeymapsDisabled: [
      "color-picker"
      "php-integrator-tooltips"
      "project-manager"
    ]
    projectHome: "~/projects"
    telemetryConsent: "limited"
    themes: [
      "spacegray-dark-neue-ui"
      "alternatone-stark-sea-syntax"
    ]
    useCustomTitleBar: true
  crystal:
    compilerColorOutput: true
    formatOnSave: true
  "crystal-block":
    highlightLineNumber: true
  "ctags-status":
    executablePath: "/usr/local/bin/ctags"
  docblockr:
    development_mode: true
    spacer_between_sections: true
  editor:
    backUpBeforeSaving: true
    fontFamily: "Fira Code"
    fontSize: 17
    invisibles: {}
    nonWordCharacters: "./\\\\()\\\"'-_:,.;<>~!@#%^&*|+=[]{}`~?"
    scrollPastEnd: true
    scrollSensitivity: 80
    showIndentGuide: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    softWrapHangingIndent: 4
    tabType: "soft"
  emmet: {}
  "exception-reporting":
    userId: "83350cd4-9f31-d095-89d1-97303c8ae8e4"
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  fonts:
    fontFamily: "Hasklig"
  "fuzzy-finder": {}
  "git-clone": {}
  "git-plus":
    general:
      _analyticsUserId: "ce446fc9-f5d8-4053-a832-bc05fd1a5ca0"
  "git-projects":
    ignoredPatterns: "node_modules;\\.git;\\.svn;vendor"
    rootPath: "/usr/local/var/www;/Users/molovo/projects"
    sortBy: "Latest modification date"
  "imdone-atom":
    openIn: {}
    todaysJournal: {}
  linter:
    errorPanelHeight: 82
    showErrorTabLine: true
    statusIconScope: "File"
  "linter-coffee-variables": {}
  "linter-jade": {}
  "linter-php": {}
  "linter-phpcs":
    executablePath: "/Users/molovo/.composer/vendor/bin/phpcs"
  "linter-phpmd":
    executablePath: "/Users/molovo/.composer/vendor/bin/phpmd"
    rulesets: "/Users/molovo/.phpmd.xml"
  "linter-sass-lint":
    configPath: "/Users/molovo/.sass-lint.yml"
  "markdown-deluxe": {}
  "markdown-preview-plus": {}
  minimap:
    absoluteMode: true
    independentMinimapScroll: true
    plugins:
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      linter: true
      linterDecorationsZIndex: 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
      "split-diff": true
      "split-diffDecorationsZIndex": 0
    scrollAnimation: true
  nuclide:
    remoteProjectsConfig: []
  "nuclide-health": {}
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "php-cs-fixer":
    allowRisky: true
    configPath: "/Users/molovo/.php_cs"
    executablePath: "/usr/local/bin/php-cs-fixer"
    executeOnSave: true
    fixerArguments: [
      "-q"
    ]
    phpExecutablePath: "/usr/local/opt/php70/bin/php"
    runPhpWithoutAnyIni: true
    showInfoNotifications: true
  "php-debug": {}
  "php-getters-setters": {}
  "php-integrator-autocomplete-plus":
    insertNewlinesForUseStatements: true
  "php-integrator-base":
    composerCommand: "/usr/local/bin/composer"
    phpCommand: "/usr/local/opt/php70/bin/php"
  "php-integrator-linter": {}
  "php-integrator-navigation": {}
  "php-integrator-symbol-viewer":
    defaultShowInherited: true
  pigments:
    extendAutocompleteToVariables: true
    markerType: "dot"
    traverseIntoSymlinkDirectories: true
  "project-manager":
    alwaysOpenInSameWindow: true
  "project-plus":
    showPath: true
  "regex-railroad-diagram": {}
  "seti-icons":
    iconsPlus: true
  "silent-sync":
    notifyMissingConfig: true
    useConfigFile: true
  "slack-chat": {}
  "smart-tab-name": {}
  "spell-check": {}
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  tabs:
    enableVcsColoring: true
  "tidy-markdown":
    ensureFirstHeaderIsH1: false
  "tidy-tabs":
    runOnSave: true
  "todo-manager":
    ignoredNames: [
      "**/node_module/**"
      "**/vendor/**"
      "**/bower_components/**"
      "**/libraries/ace/**"
      "classes/javascriptpacker.php **/libraries/whoops/**"
      "**/libraries/google/**"
      "**/libraries/mailchimp/**"
      "**/libraries/pdf/**"
      "**/libraries/rte/**"
      "**/libraries/packer/**"
      "**/libraries/less/**"
      "**/libraries/predis/**"
      "**/classes/Requests/**"
    ]
  "tool-bar":
    position: "Left"
  "tool-bar-almighty": {}
  "tree-view":
    hideIgnoredNames: true
  typewriter: {}
  welcome:
    showOnStartup: false
  "you-complete-me":
    lintDuringEdit: true
    pythonExecutable: "/usr/local/bin/python"
